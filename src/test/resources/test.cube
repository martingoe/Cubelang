import io;

fun euclidean(a: i32, b: i32): i32{
    while(b != 0){
        if (a > b)
            a = a - b;
        else
            b = b - a;
    }
    return a;
}

fun main() {
    var average = 0;
    var n = 10000000;
    for(var i = 2; i < n; i = i + 1)
        average = average + euclidean(i, n);
    printInt(average / n);
}