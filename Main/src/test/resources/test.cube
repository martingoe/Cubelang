import io;

struct ie{
    var test: i32;
    var test2: i64;
}
fun fib(n: i32): i32{
    if(n < 2) {
        return n;
    }
    return fib(n - 1) + fib(n - 2);
}

fun main() {
    var x = 1;
    var y = 2;
    var z = x + y;
    printInt(z);
    printInt(fib(30));

    var testStruct: ie;
    testStruct.test = 0;
    var testStructPointer = &testStruct;
    var testStructCopy = *testStructPointer;
    printInt(testStructCopy.test);
}

